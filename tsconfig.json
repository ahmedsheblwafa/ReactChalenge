{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
        "@components/*": [
            "./src/components/*"
          ],"@helpers/*": [
            "./src/helperFuns/*"
          ],"@interfaces/*": [
            "./src/interfaces/*"
          ],"@redux/*": [
              "./src/Redux/*"
          ]
    },
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src"
  ]
}
